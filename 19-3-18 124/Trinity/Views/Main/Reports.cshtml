<!-- BEGIN PAGE BREADCRUMBS -->
<div ng-controller="ReportsController">
    <div ng-include="'Main/ConfirmModal'"></div>
    <div class="col-lg-10 col-md-9 main-content basic-page sidebar" style="width:83%;" ng-show="!showaction">
        <h4>Report</h4>
        <div ng-show="showfilter">
            <div class="col-md-3">

                 <div class="form-group">
                    <label>Asset Class </label>
                      <multiselect placeholder="Select Asset Class..." ms-model="multiselectBusiness.selected" ms-config="multiselectBusiness.config" ms-change="assignBusiness()"
                                 ms-options="multiselectBusiness.options">
                    </multiselect>
                   @* <select class="form-control" ng-model="selectModel.Business" ng-options="item.Business for item in BusinessList" ng-change="userfilter5()" title="Select Asset Class" style="width: 100%; font-size: 0.8em" required>
                        <option value="">- Select Asset Class -</option>
                    </select>*@
                </div>

              
                <div class="form-group">
                    <label>Region</label>
                     <multiselect placeholder="Select Region..." ms-model="multiselectRegion.selected" ms-config="multiselectRegion.config" ms-change="assignregion()"
                                 ms-options="multiselectRegion.options">
                    </multiselect>
                   @* <select class="form-control" name="ddlSBU" ng-model="selectModel.Region" ng-options="item.RegionName for item in RegionMasterList" ng-change="userfilter(false)" title="Select Region" style="width:100%;font-size: 0.8em" required>
                        <option value="">- Select Region -</option>
                    </select>*@
                </div>

@*                <div class="form-group">
                    <label>Country</label>
                   
                     <multiselect placeholder="Select country..." ms-model="multiselect.selected" ms-config="multiselect.config" ms-change="assignUsers()"
                                 ms-options="multiselect.options">
                    </multiselect>
                  
                </div>*@
            </div>
            <div class="col-md-3">
              
                <div class="form-group">
                    <label>Business Line</label>
                     <multiselect placeholder="Select Business Line..." ms-model="multiselectBusinessLine.selected" ms-config="multiselectBusinessLine.config" ms-change="assignUsersBusinessLine()"
                                 ms-options="multiselectBusinessLine.options">
                    </multiselect>
                   @* <select class="form-control" name="ddlSBU" ng-model="selectModel.BusinessLine" ng-options="item.BusinessLine for item in BusinessLineList" ng-change="userfilter(false)" title="Select BusinessLine" style="width:100%;font-size: 0.8em" required>
                        <option value="">- Select BusinessLine-</option>
                    </select>*@
                </div>
                  <div class="form-group">
                    <label>LTA Application</label>
                       <multiselect placeholder="Select LTA Application..." ms-model="multiselectLTAApplication.selected" ms-config="multiselectLTAApplication.config" ms-change="assignUsers()"
                                 ms-options="multiselectLTAApplication.options">
                    </multiselect>
                  @*  <select class="form-control" name="ddlSBU" ng-model="selectModel.LTAApplicationName" ng-options="item.LTAApplicationName for item in LTAApplicationNameList" title="Select LTAApplication" style="width:100%;font-size: 0.8em" ng-change="userfilter3()" required>
                        <option value="">- Select LTA Application Name-</option>
                    </select>*@

                </div>
            </div>
            <div class="col-md-3">
                
                 <div class="form-group">
                    <label>LTA Strategy Name </label>
                      <multiselect placeholder="Select LTA Strategy..." ms-model="multiselectLTAStrategy.selected" ms-config="multiselectLTAStrategy.config" ms-change="assignltastrategyname()"
                                 ms-options="multiselectLTAStrategy.options">
                    </multiselect>

                    @*<select class="form-control" name="ddlProduct" ng-model="selectModel.LTAStrategyName" ng-options="item.LTAStrategyName for item in LTAStrategyNameList" ng-change="userfilter4()" title="Select LTAStrategyName" style="width: 100%; font-size: 0.8em" required>
                        <option value="">- Select LTA Strategy Name -</option>
                    </select>*@
                </div>
                
                 <div class="form-group">
                    <label>Priority </label>
                    <input type="text" class="form-control" maxlength="10"  ng-model="selectModel.Priority">
                </div>


            </div>
            <div class="col-md-3">

                  <div class="form-group">
                    <label>LTA Strategy Owner </label>
                       <multiselect placeholder="Select LTA Strategy Owner..." ms-model="multiselectLTAStrategyOwner.selected" ms-config="multiselectLTAStrategyOwner.config" ms-change="assignLTAStrategyOwner()"
                                 ms-options="multiselectLTAStrategyOwner.options">
                    </multiselect>
                  @*  <select class="form-control" ng-model="selectModel.LTAStrategyOwner" ng-options="item.userId for item in LTAStrategyOwnerList" title="Select LTAApplicationOwner" ng-change="checksignoff()" style="width: 100%; font-size: 0.8em" required>
                        <option value="">- Select LTA Strategy Owner -</option>
                    </select>*@
                </div>

                  <div class="form-group">
                    <label>Status </label>
                    <select class="form-control" ng-model="selectModel.Status" ng-options="item.StatusName for item in StrategyStatusType" title="Select Status" style="width: 100%; font-size: 0.8em" required>
                        <option value="">- Select Status-</option>
                    </select>
                </div>

            </div>
        </div>
        <div class="col-md-12" style="margin-bottom:10px;">
            <a target="_self" id="downloadpdf" href={{downloadurl}} download={{filename}} style="display: none"></a>
            <button data-ng-click="showfiltercondition()" class="btn btn-primary pull-right">Show Filter</button>
            <button data-ng-click="removefilter()" class="btn btn-primary pull-right">Reset Filter</button>
            <button data-ng-click="showreport()" class="btn btn-primary pull-right">Generate Report</button>
            <button data-ng-click="Export()" class="btn btn-primary pull-right">Export</button>
        </div>
        <div class="col-md-12 data-table">
            <table datatable="" dt-options="dtOptions" dt-columns="dtColumns" class="table table-responsive"></table>
        </div>
    </div>

</div>

<style>
    .{
        cursor: not-allowed;
    }

    .basic-page .form-control {
        height: 36px;
        padding: 0px 9px;
    }

    table tr {
        font-size: 14px;
    }
</style>


